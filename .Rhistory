coactivity2 <- function(beta,q=1){
C12 <- C12_f(beta)
V12 <- (C12+ t(C12))/2
Eig <- eigen(V12)
a <- rep(NA, nrow(Eig$vectors))
for(i in 1:nrow(Eig$vectors)){
a[i] <- sum(abs(Eig$values[1:q])*Eig$vectors[i,1:q]^2)
}
return(a)
}
